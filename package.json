{
	"name": "vscodejson2apex",
	"displayName": "VSCodeJSON2Apex",
	"description": "Convert JSON to Apex wrapper directly on vscode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.json2apex"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.json2apex",
				"title": "JSON2Apex"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"node-fetch": "^2.6.1",
		"python-shell": "^3.0.0"
	}
}
